<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="650dp"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/date"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:background="@drawable/table_border"
        android:padding="5dp"
        android:text="senin, 27 desember 2023"
        android:textColor="@color/text_black"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/time"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:background="@drawable/table_border"
        android:padding="5dp"
        android:text="23:23:23"
        android:textColor="@color/text_black"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/transactionID"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:background="@drawable/table_border"
        android:padding="5dp"
        android:text="ss333sdsfd444"
        android:textColor="@color/text_black"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/stockIn"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:background="@drawable/table_border"
        android:padding="5dp"
        android:text="9999"
        android:textColor="@color/text_black"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/s_purchase"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:background="@drawable/table_border"
        android:padding="5dp"
        android:text="Rp 99.999.999"
        android:textColor="@color/text_black"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/stockOut"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:background="@drawable/table_border"
        android:padding="5dp"
        android:text="9999"
        android:textColor="@color/text_black"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/remain_stock"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:background="@drawable/table_border"
        android:padding="5dp"
        android:text="9999"
        android:textColor="@color/text_black"
        android:textSize="12sp" />
</TableRow>